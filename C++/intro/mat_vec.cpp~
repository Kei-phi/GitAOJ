      cin >> M[k][l];
    }
  }

  int vec[j];
  for(int k = 0; k < j; k++){
    cin >> vec[k];
  }

  for(int k = 0; k < i; i++){
    cout << for(int l = 0; l < j; l++){int ans = 0; ans += a[k][l] * vec[l]} << endl;
  }

  return 0;
}
